<div class="sessions">
    {% for talk in talks %}
        <div class="session boss geek">
            <a name="{{ talk.sessions.id }}"></a>
            <h4>{{ talk.sessions.title }}</h4>
            {# TODO <div class="horaire">
                <span class="salle">Auditorium Lucienne et Andr√© Blin</span> 27/10/2016 <strong>09:00-09:30</strong>
            </div>#}

            <div class="details">
                <div class="abstract"><p>{{ talk.sessions.abstract }}</p></div>
                <div>
                    {% for speaker in talk[".aggregation"].speaker %}
                        <div class="conferencier">
                            <a href="/{{ event.path }}/speakers/#{{ speaker.id }}">
                                <img src="{{ photo_storage.getUrl(speaker) }}" alt="{{ speaker.firstname }} {{ speaker.lastname }}">
                            </a><br>
                            <a href="/{{ event.path }}/speakers/#{{ speaker.id }}">{{ speaker.firstname }} {{ speaker.lastname }}</a>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
    {% endfor %}
</div>
